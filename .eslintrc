{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
        {
            "files": ["*.jsx", "*.js"]
        }
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-shadow": ["off"],
        "import/no-extraneous-dependencies": ["off"],
        "react/react-in-jsx-scope": ["off"]
    }
}
